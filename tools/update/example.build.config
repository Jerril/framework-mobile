# Libraries

# Path of the libraries on your computer
libs_path="/Users/Maxime/1_Dev_Mobile/1_Libraries/"

# Path of the ANEs on your computer
anes_path="/Users/Maxime/1_Dev_Mobile/2_ANEs/"

# SDKs

# Path of the air SDK (should bo on the desktop because some files won't
# be able to be run when they are located somwhere else, such as in the
# Flash Builder 4.7 plugin folder)
air_sdk_path="/Users/Maxime/Desktop/mobile-tools/air-sdk-4.0"

# Path of the Android SDK (should be on the desktop)
android_sdk_path="/Users/Maxime/Desktop/mobile-tools/android-sdk/sdk/platforms/android-19"

# Path of the Android SDK Platform tools
android_platformtools_path="/Users/Maxime/Desktop/mobile-tools/android-sdk/sdk/platform-tools"

